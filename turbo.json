{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "dev": {
      "dependsOn": [
        "shared#build"
      ],
      "persistent": true,
      "cache": false
    },
    "clean": {
      "cache": false,
      "persistent": true
    },
    "fclean": {
      "cache": false,
      "persistent": true
    }
  }
}